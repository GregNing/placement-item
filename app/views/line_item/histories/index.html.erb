<p style="color: green"><%= notice %></p>

<div class="col-md-12">
  <h1><%= "#{@item.name} Change Histories" %></h1>
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <td>Name</td>
        <td>Booked Amount</td>
        <td>Actual Amount</td>
        <td>Adjustments</td>
        <td>Created Date</td>
      </tr>
    </thead>
    <tbody>
      <% @histories.each do |history| %>
        <%= render 'history', history: history %>
      <% end %>
    </tbody>
  </table>
</div>
<br>

<div class="btn-group" role="group" aria-label="Basic outlined example">
  <%= link_to 'Back To Line Item', campaign_path(@item.campaign), class: 'btn btn-outline-primary' %>
</div>
